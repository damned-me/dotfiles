! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #a54242
*.color9:       #cc6666

! green
*.color2:       #8c9440
*.color10:      #b5bd68

! yellow
*.color3:       #de935f
*.color11:      #f0c674

! blue
*.color4:       #5f819d
*.color12:      #81a2be

! magenta
*.color5:       #85678f
*.color13:      #b294bb

! cyan
*.color6:       #5e8d87
*.color14:      #8abeb7

! white
*.color7:       #707880
*.color15:      #c5c8c

! Fonts
*faceName: DejaVu Sans Mono 
*font: DejaVu Sans Mono
! for Powerline 
*faceSize: 14 

! Clipboard
*translations: #override <Btn1Up>: select-end(PRIMARY, CLIPBOARD, CUT_BUFFER0)

*Translations: #override \
                 Ctrl Shift <Key>V:    insert-selection(SELECT) \n\
                 Ctrl Shift <Key>C:    copy-selection(SELECT)

! Scrollback...
*savelines: 16384
! do not scroll with output
*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
*scrollWithBuffer: true

! scroll back to the bottom on keypress
*scrollTtyKeypress: true

! double-click to select whole URLs
*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48
